#%RAML 1.0 DataType
displayName: ErrorResponseType
description: Defines the message body returned with error responses
properties:
  errorCode:
    type: string
    required: true
    description: "Short code to identify the error"
    example: "INVALID_PARAMETER"
  errorMessage:
    type: string
    required: true
    description: "Verbose human readable error message"
    example: "The value you entered is out of bounds"
  transactionId:
    type: string
    required: true
    description: "Request identifier"
    example: "dd960082-abbf-47c2-8c65-f18eeb35848b"
  timeStamp:
    type: datetime
    required: true
    description: "event date time"
    example: "2019-09-23T11:46:44.256-05:00"
